<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.shape.SVGPath?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:id="component">

    <Group fx:id="svgGroup" onMouseEntered="#displayText">
        <SVGPath content="M 20 20 L 20 80 L 80 80 L 80 20 L 20 20 z" styleClass="component"/>
        <Text text="U" fx:id="text" x="50" y="50" styleClass="gateText" id="outputText"/>
    </Group>

</AnchorPane>

