<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.Group?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.shape.Line?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="javafx.component.ComponentController"
            fx:id="component">
    <VBox fx:id="componentVBox">
        <Group fx:id="svgGroup" onMouseClicked="#deleteImageAsExample">
            <Rectangle x ="20" y="20" width="5" height="60"/>

            <Line startX="21" startY="20" endX="70" endY="50" strokeWidth="5"/>

            <Line startX="21" startY="80" endX="70" endY="50" strokeWidth="5"/>
        </Group>
    </VBox>

</AnchorPane>
