<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      prefWidth="200"
      spacing="10">
    <padding>
        <Insets bottom="5" right="5" left="5" top="5"/>
    </padding>
    <Text text="Boolean logic value AND&#10;&#10;True only if all of the inputs are True" styleClass="descriptionText" wrappingWidth="190"/>
    <TableView>
        <columns>
            <TableColumn text="x" prefWidth="50"/>
            <TableColumn text="y" prefWidth="50"/>
            <TableColumn text="output" prefWidth="90"/>
        </columns>
    </TableView>
</VBox>
