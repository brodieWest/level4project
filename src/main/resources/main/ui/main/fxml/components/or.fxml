<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.Group?>
<?import javafx.scene.shape.SVGPath?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:id="component" prefHeight="100" prefWidth="100">

        <Group fx:id="svgGroup"
               onMouseEntered="#displayText"
               onMousePressed="#findLocation"
               onMouseDragged="#moveComponent"
               onMouseReleased="#stopMoving">
            <SVGPath content="M 20 20 A 10 30 0 0 1 20 80 A 55 30 0 0 0 20 20" styleClass="component, gate"/>

            <!--Rectangle x ="20" y="20" width="5" height="60"/>

            <Arc centerX="25" centerY="50" radiusX="50" radiusY="30" startAngle="-90" length="180"/>

            <Arc centerX="20" centerY="50" radiusX="15" radiusY="30" startAngle="-90" length ="180" fill="white"/-->
        </Group>


</AnchorPane>
